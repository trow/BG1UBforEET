//IKE changes
ADD_TRANS_TRIGGER IKE 0 ~ReactionGT(LastTalkedToBy,NEUTRAL_UPPER)~ DO 0

EXTEND_BOTTOM IKE 0
  IF ~ReactionLT(LastTalkedToBy,FRIENDLY_LOWER)
ReactionGT(LastTalkedToBy,HOSTILE_UPPER)~ THEN GOTO 2
  IF ~ReactionLT(LastTalkedToBy,NEUTRAL_LOWER)~ THEN GOTO 3
END

ADD_TRANS_TRIGGER IKE 1 ~PartyGoldGT(59)~ DO 0
ADD_TRANS_TRIGGER IKE 2 ~PartyGoldGT(79)~ DO 0
ADD_TRANS_ACTION IKE BEGIN 2 END BEGIN 0 END ~TakePartyGold(80)~
ADD_TRANS_TRIGGER IKE 3 ~PartyGoldGT(99)~ DO 0
ADD_TRANS_ACTION IKE BEGIN 3 END BEGIN 0 END ~TakePartyGold(100)~
ADD_TRANS_TRIGGER IKE 4 ~PartyGoldGT(59)~ DO 0
ADD_TRANS_TRIGGER IKE 4 ~PartyGoldGT(79)~ DO 3
REPLACE_TRANS_ACTION IKE BEGIN 4 END BEGIN 3 END ~TakePartyGold(60)~ ~TakePartyGold(80)~
ADD_TRANS_TRIGGER IKE 4 ~PartyGoldGT(99)~ DO 4
REPLACE_TRANS_ACTION IKE BEGIN 4 END BEGIN 4 END ~TakePartyGold(60)~ ~TakePartyGold(100)~

